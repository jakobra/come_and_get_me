<h1>New track</h1>

<% form_for(@track, :html => {:multipart => true}) do |f| %>
	<%= f.error_messages %>
	
	<p>
		<%= f.label :name %><br />
		<%= f.text_field :name %>
	</p>

	<p>
		<%= f.label :track %><br />
		<%= f.file_field :track %>
	</p>
	
	<p>
		<%= f.submit 'Create' %>
	</p>
	
<% end %>

<%= link_to 'Back', tracks_path %>