<% content_for :left_side do %>
	<% @current_menu_node.menu_node_side_modules.left_after.each do |menu_node_side_module| %>
		<%= render_html_content menu_node_side_module.side_module %>
	<% end %>
<% end %>

<% content_for :right_side do %>
	<% @current_menu_node.menu_node_side_modules.right_after.each do |menu_node_side_module| %>
		<%= render_html_content menu_node_side_module.side_module %>
	<% end %>
<% end %>